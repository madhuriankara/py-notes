   "source": [
    "Takes two variables.\n",
    "Swaps their values without using a third variable.\n",
    "Prints the swapped values."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 27,
   "id": "8f5eaf06-d171-4f62-8514-fc2a40dff678",
   "metadata": {},
   "outputs": [],
   "source": [
    "def swap_two_variables():\n",
    "    a=10\n",
    "    b=20\n",
    "    a,b = b,a\n",
    "    result = (a,b)\n",
    "    return f'The {result} is now swapped'\n",
    "    "
   ]
  },
