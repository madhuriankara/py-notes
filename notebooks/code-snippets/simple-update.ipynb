   "outputs": [],
   "source": [
    "# Build an UPDATE query\n",
    "query = Query.update(users).set(users.name, \"Jane Doe\").where(users.id == 1)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 11,
   "id": "fec2776a-8448-48d4-8a01-3b929202e1ce",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "UPDATE \"users\" SET \"name\"='Jane Doe' WHERE \"id\"=1\n"
     ]
    }
   ],
   "source": [
    "print(query)"
   ]
